{
  "name": "cv-justin-rankin",
  "version": "1.9.0",
  "description": "CV / Curriculum Vitae :: Justin Blair Rankin",
  "main": "index.js",
  "author": {
    "name": "Justin Blair Rankin",
    "email": "justin.blair.rankin@gmail.com",
    "url": "https://finografic.github.io/cv-justin-rankin"
  },
  "copyright": "Copyright 2020 finografic.com",
  "license": "ISC",
  "private": false,
  "scripts": {
    "start": "npm run dev:start",
    "start:scripts": "npm run dev:consoles && npm run dev:start",
    "·········· DEV": "··················································",
    "dev:start": "react-app-rewired start",
    "dev:start:https": "HTTPS=true react-app-rewired start",
    "dev:start:lint": "concurrently \"yarn lint:watch\" \"yarn start\"",
    "dev:watch": "nodemon --quiet --exec 'react-app-rewired start'",
    "dev:consoles": "./scripts/open-konsoles.sh",
    "dev:browser": "nodemon ./scripts/development.js",
    "serve": "react-app-rewired build && serve build",
    "·········· BUILD": "··················································",
    "build": "GENERATE_SOURCEMAP=false react-app-rewired build",
    "clean": "rimraf ./node_modules && rimraf ./build rm yarn.lock && yarn",
    "clean:build": "rimraf ./build",
    "clean:all": "npm run test:clean && npm run clean:build",
    "·········· DEPLOY": "··················································",
    "deploy": "NODE_DEBUG=gh-pages gh-pages --dist build",
    "deploy:full": "npm run build && npm run deploy",
    "deploy:help": "gh-pages --help",
    "·········· LINTING": "··················································",
    "lint": "esw --color",
    "lintORIG": "npm run lint:js && npm run lint:css",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern ./scripts",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern ./scripts --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:css": "stylelint app/**/*.js",
    "lint:watch": "esw --watch --color",
    "lint:watch:fix": "esw --color --fix",
    "lint:staged": "lint-staged",
    "prettify": "prettier --write .",
    "·········· TESTING:": "··················································",
    "test": "react-app-rewired test — env=jsdom",
    "test:jest": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "checknodenpmversions": "node ./scripts/checkNodeNpmVersions.js",
    "·········· HOOKS": "··················································",
    "preinstall": "npm run checknodenpmversions",
    "prebuild": "npm run clean:build",
    "pretest": "npm run test:clean && npm run lint",
    "predeploy": "rimraf ./build && npm run build && GENERATE_SOURCEMAP=false react-app-rewired build",
    "···················": "··················································",
    "                   ": "                                                  "
  },
  "scripts-TEST": {
    "lint:test": "prettier --check  \"src/**/*.js\" \"src/**/*.jsx\"",
    "lint:write": "prettier --write \"src/**/*.js\" \"src/**/*.jsx\"",
    "delete-maps": "yarn run delete-map-files && yarn run delete-references-to-map-files",
    "delete-map-files": "find ./build -name '*.map' -delete",
    "delete-references-to-map-files": "find ./build -regex '.*\\.\\(js\\|css\\)' -exec sed -i -E '\\/[\\*\\/]#\\ssourceMappingURL=main(\\.[0-9a-f]+)?\\.(css|js)\\.map(\\*\\/)?/g' {} +",
    "build:finografic": "GENERATE_SOURCEMAP=false react-scripts build && node minify.js && rm finografic.com -fr && cp -r build finografic.com",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@emotion/core": "^10.0.35",
    "@emotion/react": "^11.0.0-next.15",
    "@emotion/styled": "^10.0.27",
    "@formatjs/intl-displaynames": "^1.2.9",
    "@loadable/component": "^5.13.2",
    "@reach/router": "^1.3.3",
    "@reduxjs/toolkit": "^1.1.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "babel-plugin-react-intl": "^8.2.7",
    "full-icu": "^1.3.1",
    "node-fetch": "^2.6.0",
    "polished": "^4.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-bootstrap": "^1.3.0",
    "react-device-detect": "^1.12.1",
    "react-dom": "^16.13.1",
    "react-helmet-async": "^1.0.7",
    "react-icons": "^3.11.0",
    "react-intl": "^4.5.3",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "react-scroll": "^1.8.1",
    "react-spinners": "^0.9.0",
    "react-sticky": "^6.0.3",
    "react-styled-flexboxgrid": "^3.2.1",
    "sanitize.css": "^12.0.1",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.11.1",
    "concurrently": "^5.2.0",
    "customize-cra": "^1.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-i18n-json": "^2.4.4",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "eslint-watch": "^7.0.0",
    "gh-pages": "^3.1.0",
    "lint-staged": "^10.2.13",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "offline-plugin": "^5.0.7",
    "pre-commit": "^1.2.2",
    "prettier": "^2.1.1",
    "react-app-rewired": "^2.1.6",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "serve": "^11.3.2",
    "stylelint": "^13.6.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "yarn": "^1.22.4",
    "yarn-upgrade-all": "^0.5.4"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "yarn-upgrade-all": {
    "ignore": [
      "eslint",
      "history"
    ]
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/finografic/cv-justin-rankin.git"
  },
  "proxy": "http://localhost:3000",
  "homepage": "https://finografic.github.io/cv-justin-rankin",
  "keywords": [
    "CV",
    "curriculum vitae",
    "Justin Rankin",
