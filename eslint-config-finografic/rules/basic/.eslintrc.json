{
  "rules": {
    "eqeqeq": [2, "smart"],
    "semi": 0,
    "quotes": ["error", "single"],
    "one-var": 0,
    "no-unused-vars": 0,
    "max-len": 0,
    "arrow-parens": 0,
    "no-console": 0,
    "no-param-reassign": 0,
    "no-multiple-empty-lines": 0,
    "block-spacing": 2,
    "comma-dangle": 0,
    "comma-style": 2,
    "eol-last": 2
  }
}
